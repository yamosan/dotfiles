[user]
	name = yamori-masato
	email = yamorimasato@gmail.com
[core]
	excludesfile = /Users/yamori/.gitignore_global
[alias]
	aliases = config --get-regexp ^alias\\.
	br = branch
	st = status
	co = checkout
	com = commit
	ignore = "!gi() { curl -sL https://www.toptal.com/developers/gitignore/api/$@ > .gitignore ;}; gi"
	lga = log --graph --abbrev-commit --decorate --format=format:'%C(bold blue)%h%C(reset) - %C(bold green)(%ar)%C(reset) %C(white)%s%C(reset) %C(dim white)- %an%C(reset)%C(bold yellow)%d%C(reset)' --all
	lg = lga -15
  reamend = commit --amend -C HEAD
